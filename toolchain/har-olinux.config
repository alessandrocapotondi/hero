# General
CT_CONFIG_VERSION="3"
CT_EXPERIMENTAL=y
CT_RM_RF_PREFIX_DIR=n
CT_TARGET_VENDOR="none"

# Arch
CT_ARCH_ARM=y
CT_DEMULTILIB=y
CT_ARCH_USE_MMU=y
CT_ARCH_64=y

# Binutils
CT_BINUTILS_V_2_32=y

# Libc
CT_LIBC_GLIBC=y
CT_GLIBC_V_2_29=y
CT_GLIBC_KERNEL_VERSION_CHOSEN=y
CT_GLIBC_MIN_KERNEL_VERSION="4.13.0"

# Compiler
CT_GCC_V_8=y
CT_CC_LANG_CXX=y

# Linux
CT_KERNEL_LINUX=y
CT_LINUX_V_4_14=y
